{
  "compilerOptions": {
    "target": "es5",
    "lib": ["es5", "es6", "dom"],
    "types": ["cypress", "node"],
    "moduleResolution": "node",
    "esModuleInterop": true,
    "skipLibCheck": true,
    "module": "commonjs",
    "strict": false,
  },
  "include": [
    "**/*.ts",
    "cypress/**/*.ts", 
    "cypress/**/*.tsx"
  ]
}
